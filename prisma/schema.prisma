generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "sqlite"
  url      = "file:akeneo.db"
}

model events {
  id            Int             @id @default(autoincrement())
  created_at    DateTime        @default(now())
  content       String
  event_details event_details[]
}

model event_details {
  id         Int      @id @default(autoincrement())
  created_at DateTime @default(now())
  event      events   @relation(fields: [event_id], references: [id])
  event_id   Int
  content    String
}
